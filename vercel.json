{
  "rewrites": [
    {
      "source": "/create-association/:id?",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=UTF-8"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=UTF-8"
        }
      ]
    },
    {
      "source": "/(.*).(svg|png|jpg|jpeg)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/*"
        }
      ]
    },
    {
      "source": "/(.*).woff2?",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/woff2"
        }
      ]
    },
    {
      "source": "/(.*).ttf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/ttf"
        }
      ]
    }
  ]
}
