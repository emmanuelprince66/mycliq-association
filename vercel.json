{
  "rewrites": [
    {
      "source": "/create-association/:id",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "file" }],
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).(js|css|svg|png|jpg|woff2?|ttf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/javascript; charset=UTF-8"
        }
      ]
    }
  ]
}
